<?xml version=3D"1.0" encoding=3D"UTF-8"?>
<xsl:stylesheet version=3D"1.0" =
xmlns:xsl=3D"http://www.w3.org/1999/XSL/Transform">
<xsl:output method=3D"html"/>

<!-- Parameters -->
<xsl:param name=3D"action"/>
<xsl:param name=3D"procedureChanged"/>
<xsl:param name=3D"reportExists"/>

<!-- Local Specific Default Values -->
<xsl:variable name=3D"lang.procedure.type" select=3D"'Procedure =
Type'"/>
<xsl:variable name=3D"lang.procedure.location" select=3D"'Location'"/>
<xsl:variable name=3D"lang.procedure.date" select=3D"'Date'"/>
<xsl:variable name=3D"lang.procedure.dateTT" select=3D"'Click on =
calendar to select the date.'"/>
<xsl:variable name=3D"lang.patient.Surgeon" select=3D"'Surgeon'"/>
<xsl:variable name=3D"lang.procedure.PreOpDX" select=3D"'Preoperative =
Diagnosis'"/>
<xsl:variable name=3D"lang.procedure.required" select=3D"'Required =
Fields'"/>
<xsl:variable name=3D"lang.procedure.create" select=3D"'Create'"/>
<xsl:variable name=3D"lang.procedure.clear" select=3D"'Clear Fields'"/>
<xsl:variable name=3D"lang.procedure.update" select=3D"'Save'"/>
<xsl:variable name=3D"lang.procedure.reset" select=3D"'Restore =
Fields'"/>

<!---This Xsl deals with a doc that is a merger of procedure and user =
xmls, the root being the list
-->
<xsl:template match=3D"List">

   <xsl:apply-templates select=3D"Procedure"/>

   <table align=3D"center">
      <tr><td colspan =3D "3"><HR></HR></td></tr>
      <xsl:choose>
        <xsl:when test=3D"$action=3D'create' and =
$procedureChanged=3D'changed'">
           <tr>
             <td><input id=3D"SaveButton" class=3D"BGGrey" =
type=3D"button" value=3D"{$lang.procedure.create}"
                =
onClick=3D"checkProcedureRequiredFields('{$action}');"/></td>
             <td><input id=3D"ResetButton" class=3D"BGGrey" =
type=3D"reset" onClick=3D"resetDate();" =
value=3D"{$lang.procedure.clear}"/></td>
           </tr>
        </xsl:when>
        <xsl:otherwise>
      <tr>
         <td><input id=3D"SaveButton" class=3D"BGGrey" type=3D"button" =
value=3D"{$lang.procedure.update}"
                                    =
onClick=3D"checkProcedureRequiredFields('update');"/></td>
         <td><input id=3D"ResetButton" class=3D"BGGrey" type=3D"reset" =
onClick=3D"resetDate();" value=3D"{$lang.procedure.reset}"/></td>
        <td><input id=3D"GotoReportButton" class=3D"BGGrey" =
type=3D"button" value=3D"{$reportExists}"
                    =
onClick=3D"goToOpNotes({Procedure/@patientId},{Procedure/@procedureId});=
"/>
        </td>
    </tr>
    </xsl:otherwise>
    </xsl:choose>
   <tr><td colspan =3D "3"><HR></HR></td></tr>
  </table>
</xsl:template>

<xsl:template match=3D"Procedure">
    <input type=3D"hidden" name=3D"procedureId" =
value=3D"{@procedureId}"/>
    <input type=3D"hidden" name=3D"patientId" value=3D"{@patientId}"/>
    <input type=3D"hidden" name=3D"procedureChanged"/>

    <table align=3D"center" class=3D"searchBorder">
      <tr>
        <td class=3D'boldSmallFont'><xsl:value-of =
select=3D"$lang.procedure.type"/><xsl:text> * </xsl:text></td>
        <td class=3D'smallFont'>
          <select id=3D"ProcName" Name=3D"procedureNameId" =
onchange=3D"changeDiagnosis(this)">
            <xsl:for-each select=3D"../ProcedureName">
              <xsl:choose>
                <xsl:when test=3D"(@procedureNameId =3D =
/List/Procedure/@procedureNameId)">
                  <option value=3D"{@procedureNameId}" =
SELECTED=3D"true"><xsl:value-of select=3D"DisplayName"/></option>
                </xsl:when>
                <xsl:otherwise>
                  <option value=3D"{@procedureNameId}"><xsl:value-of =
select=3D"DisplayName"/></option>
                </xsl:otherwise>
              </xsl:choose>
            </xsl:for-each>
          </select>
        </td>
      </tr>
  <tr>
    <td class=3D'boldSmallFont'><xsl:value-of =
select=3D"$lang.procedure.location"/></td>
    <td>
      <select id=3D"Location" name=3D"Location" SIZE=3D"1">
        <xsl:variable name=3D"curLocation" =
select=3D"ProcedureData/Location"/>
        <xsl:choose>
          <xsl:when test=3D"normalize-space($curLocation)">
            <option>
              <xsl:text> </xsl:text>
            </option>
          </xsl:when>
          <xsl:otherwise>
            <option selected=3D"true">
              <xsl:text> </xsl:text>
            </option>
          </xsl:otherwise>
        </xsl:choose>
        <xsl:apply-templates select=3D"../LocationList/Location">
          <xsl:with-param name=3D"curLocation" =
select=3D"$curLocation"/>
        </xsl:apply-templates>
      </select>
    </td>
  </tr>

  <tr>
  <td class=3D'boldSmallFont'><xsl:value-of =
select=3D"$lang.patient.Surgeon"/><xsl:text> * </xsl:text></td>
  <td>
    <xsl:variable name=3D"userId" select=3D"@userId"/>
    <select name=3D"surgeonId" SIZE=3D"1">
      <xsl:for-each select=3D"User">
      <xsl:sort data-type=3D"text" select=3D"Name/LastName" =
order=3D"ascending"/>
        <xsl:variable name=3D"curUser" select=3D"@userId"/>
        <xsl:choose>
          <xsl:when test=3D"$curUser =3D $userId">
            <option VALUE=3D"{$curUser}" selected=3D"true" =
defaultSelected=3D"true">
              <xsl:value-of select=3D"Name/FirstName"/>
              <xsl:text> </xsl:text>
              <xsl:if test=3D"string-length(Name/MI) &gt; 0">
                <xsl:value-of select=3D"Name/MI"/><xsl:text>. =
</xsl:text>
              </xsl:if>
              <xsl:value-of select=3D"Name/LastName"/>
              <xsl:if test=3D"string-length(Name/Title) &gt; 0">
                <xsl:text>, </xsl:text>
                <xsl:value-of select=3D"Name/Title"/>
              </xsl:if>
            </option>
          </xsl:when>
          <xsl:otherwise>
            <option VALUE=3D"{$curUser}">
              <xsl:value-of select=3D"Name/FirstName"/>
              <xsl:text> </xsl:text>
              <xsl:if test=3D"string-length(Name/MI) &gt; 0">
                <xsl:value-of select=3D"Name/MI"/><xsl:text>. =
</xsl:text>
              </xsl:if>
              <xsl:value-of select=3D"Name/LastName"/>
              <xsl:if test=3D"string-length(Name/Title) &gt; 0">
                <xsl:text>, </xsl:text>
                <xsl:value-of select=3D"Name/Title"/>
              </xsl:if>
            </option>
          </xsl:otherwise>
        </xsl:choose>
      </xsl:for-each>
    </select>
  </td>
</tr>



 <tr>
    <td class=3D'boldSmallFont'>
        <xsl:value-of select=3D"$lang.procedure.date"/><xsl:text> * =
</xsl:text>
    </td>

 <td>
 <table>
    <td>
        <label for=3D"calDate" id=3D"labelDate" class=3D'arialFont' =
title=3D"{$lang.procedure.dateTT}">
               <xsl:value-of select=3D"Date"/>
        </label>
        <input type=3D"hidden" id=3D"calDate" name=3D"Date" =
value=3D"{Date}"/>
    </td>

    <td>
        <script language=3D"JavaScript">
                var cal2 =3D new =
calendar2(procedureForm.Date,labelDate);
                cal2.year_scroll =3D true;
                cal2.time_comp =3D false;
        </script>

        <div onClick=3D"cal2.popup()" tabindex=3D"0">
        <img src=3D"images/cal.gif" width=3D"26"
           height=3D"26" border=3D"0" =
alt=3D"{$lang.procedure.dateTT}"/>
        </div>
    </td>
 </table>
 </td>
 </tr>

  <xsl:apply-templates select=3D"ProcedureData"/>
  <tr>
    <td class=3D"smallFont">
        <xsl:text>*</xsl:text>
        <xsl:value-of select=3D"$lang.procedure.required" />
    </td>
  </tr>
</table>

    <br/>

</xsl:template>

  <xsl:template match=3D"Location">
    <xsl:param name=3D"curLocation"/>
    <xsl:choose>
      <xsl:when test=3D". =3D $curLocation">
        <option selected=3D"true">
          <xsl:value-of select=3D"."/>
        </option>
      </xsl:when>
      <xsl:otherwise>
        <option>
          <xsl:value-of select=3D"."/>
        </option>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>


<xsl:template match=3D"ProcedureData">
 <tr>
    <td class=3D'boldSmallFont'><xsl:value-of =
select=3D"$lang.procedure.PreOpDX"/><xsl:text> * </xsl:text></td>
  </tr>
  <tr>
    <xsl:choose>
      <xsl:when test=3D"string-length(normalize-space(PreAssessment)) =
&gt; 0">
        <td colspan=3D"2" class=3D'boldSmallFont'>
        <textarea name=3D"PreAssessment" rows=3D"5" cols=3D"50" =
maxlength=3D"1000">
          <xsl:value-of select=3D"PreAssessment"/></textarea></td>
      </xsl:when>
      <xsl:otherwise>
        <td colspan=3D"2" class=3D'boldSmallFont'><textarea =
name=3D"PreAssessment" rows=3D"5" cols=3D"50" maxlength=3D"1000" =
></textarea></td>
      </xsl:otherwise>
    </xsl:choose>
  </tr>
</xsl:template>

</xsl:stylesheet>

